<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.release.fast">

    <application
        android:name="com.release.fast.App"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:networkSecurityConfig="@xml/network_security_config"
        android:theme="@style/AppTheme">
        <activity android:name="com.release.fast.ui.act.SplashActivity"
            android:theme="@style/AppLauncher">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity android:name="com.release.fast.ui.act.LoginActivity"
            android:launchMode="singleTop"
            />
        <activity android:name="com.release.fast.MainActivity"
            android:launchMode="singleTask"
            />
        <activity android:name="com.release.fast.ui.act.WebActivity"
            android:launchMode="singleTop"
            />
    </application>

</manifest>